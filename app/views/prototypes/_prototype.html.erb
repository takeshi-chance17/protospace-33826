<div class="card">
  <%# prototypeを複数回表示 %>
  <% prototype.length.times do |i| %>
    <%= link_to image_tag( prototype[i].image, class: :card__img ), root_path %>
    <div class="card__body">
      <%= link_to prototype[i].title, root_path, class: :card__title %>
      <p class="card__summary">
        <%= prototype[i].catch_copy %>
      </p>
      <%= link_to current_user.name, root_path, class: :card__user %>
    </div>
  <% end %>
</div>