<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
    <%# ログインしているときは以下を表示する %>
      <div class="greeting">
        <span>こんにちは、</span>
        <%= link_to current_user.name, user_path(current_user.id), class: :greeting__link, id: :hello_name %>
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: "prototype", locals: { prototype: @prototypes } %>
    </div>
  </div>
</main>
